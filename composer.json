{
    "name": "wabondpro/wabondpro-php",
    "description": "Client Wrapper for Wabond Pro",
    "keywords": [
        "WabondPro",
        "wabondpro",
        "wabond-client"
    ],
    "homepage": "https://github.com/wabondpro/wabondpro-php",
    "license": "MIT",
    "authors": [
        {
            "name": "ricardosawir",
            "email": "sawir.ricardo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1||^8.0",
        "guzzlehttp/guzzle": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.28",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "WabondPro\\WabondClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WabondPro\\WabondClient\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
